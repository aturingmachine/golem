{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "homebrew discord bot",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rm -rf dist/ && npm run build",
    "dev": " npm run build:clean && NODE_OPTIONS=--enable-source-maps node  dist/index.js",
    "dev:debug": "npm run dev debug tty",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "test": "NODE_ENV=test jest",
    "cmd:reg": "npm run build && node dist/deploy-commands.js -- noRun",
    "cmd:dmp": "node ./scripts/dump-commands.js",
    "docs:dev": "npm run cmd:dmp && npm run dev:gen --prefix ./docs",
    "docs:build": "npm run build --prefix ./docs",
    "docs:deploy": "sh ./scripts/docs-deploy.sh"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/testing": "^8.4.7",
    "@types/common-tags": "^1.8.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.0",
    "@types/md5": "^2.3.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.5",
    "@types/sharp": "^0.29.0",
    "@types/terminal-kit": "^1.28.4",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.5.0",
    "jest-mock": "^29.4.3",
    "prettier": "^2.3.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.15.0",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/microservices": "^8.4.7",
    "@nestjs/typeorm": "^9.0.0",
    "axios": "^0.21.4",
    "chalk": "^4.1.2",
    "common-tags": "^1.8.2",
    "dargs": "^7.0.0",
    "discord.js": "^14.8.0",
    "dotenv": "^10.0.0",
    "execa": "^5.1.1",
    "express": "^4.17.1",
    "fast-average-color-node": "^1.0.3",
    "ffmpeg-static": "^4.4.0",
    "form-data": "^4.0.0",
    "fuse.js": "^6.4.6",
    "fuzzy": "^0.1.3",
    "libsodium-wrappers": "^0.7.9",
    "md5": "^2.3.0",
    "minimist": "^1.2.6",
    "music-metadata": "^7.11.4",
    "node-cron": "^3.0.2",
    "opusscript": "^0.0.8",
    "plex-api": "^5.3.2",
    "programmatic-repl": "^1.1.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "serverline": "^1.5.0",
    "sharp": "^0.29.1",
    "terminal-kit": "^2.1.6",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yaml": "2.0.0-9",
    "ytdl-core": "^4.9.1",
    "ytpl": "^2.2.3",
    "ytsr": "^3.8.0"
  }
}
